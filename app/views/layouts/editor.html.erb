<!DOCTYPE html>
<html>
<head>
  <title>Dashboardi</title>
  <%= stylesheet_link_tag    "editor", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "editor", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <div id="editor">
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, id: "flash_#{name}" %>
      <% end %>
      <%= yield %>
    </div>
<div id="preloadedImages">
  <% @palette.each do |pitem| %>
    <img src='<%= image_path('panels/' + pitem.name + '.png') %>' alt=''></img>
  <% end %>

</div>
</body>
</html>
